<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="816.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.cl3t4p.progetto.lavoratori2022.fx.controllers.AggLavOPController">
    <children>
        <Label layoutX="14.0" layoutY="22.0" text="ID:"/>
        <TableView fx:id="patenti_view" layoutX="706.0" layoutY="45.0" prefHeight="339.0" prefWidth="102.0">
            <columns>
                <TableColumn fx:id="patente_colum" prefWidth="75.0" text="PATENTI"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <Label fx:id="label_id" layoutX="35.0" layoutY="20.0" prefHeight="17.0" prefWidth="48.0" underline="true">
            <font>
                <Font size="13.0"/>
            </font>
        </Label>
        <Button layoutX="706.0" layoutY="17.0" mnemonicParsing="false" onAction="#addPatente" prefHeight="25.0"
                prefWidth="25.0" text="+"/>
        <ChoiceBox fx:id="patente" layoutX="734.0" layoutY="17.0" prefHeight="25.0" prefWidth="74.0"/>
        <TableView fx:id="esp_view" layoutX="120.0" layoutY="45.0" prefHeight="147.0" prefWidth="360.0">
            <columns>
                <TableColumn fx:id="esp_col" text="ESPERIENZE"/>
            </columns>
        </TableView>
        <TableView fx:id="lig_view" layoutX="120.0" layoutY="237.0" prefHeight="147.0" prefWidth="360.0">
            <columns>
                <TableColumn fx:id="lig_col" text="LINGUE"/>
            </columns>
        </TableView>
        <Button layoutX="91.0" layoutY="17.0" mnemonicParsing="false" onAction="#addEsp" prefHeight="25.0"
                prefWidth="25.0" text="+"/>
        <Button layoutX="91.0" layoutY="209.0" mnemonicParsing="false" onAction="#addLingua" prefHeight="25.0"
                prefWidth="25.0" text="+"/>
        <TextField fx:id="esperienze" layoutX="120.0" layoutY="17.0" prefHeight="25.0" prefWidth="360.0"/>
        <TextField fx:id="lingue" layoutX="120.0" layoutY="209.0" prefHeight="25.0" prefWidth="360.0"/>
        <Button layoutX="14.0" layoutY="346.0" mnemonicParsing="false" onAction="#back" prefHeight="38.0"
                prefWidth="62.0" text="Back"/>
        <TableView fx:id="comuni_view" layoutX="485.0" layoutY="45.0" prefHeight="339.0" prefWidth="215.0">
            <columns>
                <TableColumn fx:id="comuni_col" prefWidth="75.0" text="COMUNI"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <Button layoutX="485.0" layoutY="17.0" mnemonicParsing="false" onAction="#addComune" prefHeight="25.0"
                prefWidth="25.0" text="+"/>
        <ComboBox fx:id="comune" editable="true" layoutX="513.0" layoutY="17.0" onKeyReleased="#comune_search"
                  prefHeight="25.0" prefWidth="186.0"/>
    </children>
</AnchorPane>
